{
  "name": "awslambdahackathon",
  "title": "AWS Lambda Hackathon",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "eslint \"apps/**/*.{ts,tsx}\" \"packages/**/*.{ts,tsx}\" --ignore-pattern \"**/cdk.out/**\" --ignore-pattern \"**/dist/**\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "deploy": "sh scripts/deploy.sh",
    "destroy": "sh scripts/destroy.sh",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.0.0",
    "@aws-lambda-powertools/tracer": "^2.0.0",
    "@aws-lambda-powertools/metrics": "^2.0.0",
    "@middy/core": "^5.0.0",
    "@middy/http-json-body-parser": "^5.0.0",
    "@middy/http-error-handler": "^5.0.0",
    "@middy/http-cors": "^5.0.0",
    "@middy/validator": "^5.0.0",
    "zod": "^3.22.0",
    "aws-cdk-lib": "^2.202.0",
    "constructs": "^10.0.0",
    "esbuild": "^0.23.0",
    "fs-extra": "^11.2.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "aws-cdk": "2.1019.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "turbo": "^2.0.0",
    "typescript": "^5.4.0"
  },
  "packageManager": "npm@10.2.0",
  "engines": {
    "node": ">=18.0.0"
  }
}
