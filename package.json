{
  "name": "awslambdahackathon",
  "title": "AWS Lambda Hackathon",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "eslint \"apps/**/*.{ts,tsx}\" \"packages/**/*.{ts,tsx}\" --ignore-pattern \"**/cdk.out/**\" --ignore-pattern \"**/dist/**\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "deploy": "sh scripts/deploy.sh",
    "destroy": "sh scripts/destroy.sh",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.1.12",
    "@aws-lambda-powertools/logger": "^2.0.0",
    "@aws-lambda-powertools/metrics": "^2.0.0",
    "@aws-lambda-powertools/tracer": "^2.0.0",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.835.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.596.0",
    "@aws-sdk/client-dynamodb": "^3.529.1",
    "@aws-sdk/client-secrets-manager": "^3.596.0",
    "@aws-sdk/lib-dynamodb": "^3.529.1",
    "@middy/core": "^5.0.0",
    "@middy/http-cors": "^5.0.0",
    "@middy/http-error-handler": "^5.0.0",
    "@middy/http-json-body-parser": "^5.0.0",
    "@middy/validator": "^5.0.0",
    "amazon-cognito-identity-js": "^6.2.1",
    "aws-amplify": "^6.4.0",
    "aws-cdk-lib": "^2.202.0",
    "aws-jwt-verify": "^4.0.0",
    "aws-rum-web": "^1.24.0",
    "constructs": "^10.0.0",
    "esbuild": "^0.23.0",
    "fs-extra": "^11.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.24.1",
    "serverless": "^3.38.0",
    "serverless-plugin-typescript": "^2.1.5",
    "source-map-support": "^0.5.21",
    "ws": "^8.14.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.129",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.17",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.0",
    "aws-cdk": "2.1019.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "turbo": "^2.0.0",
    "typescript": "^5.4.0",
    "vite": "^5.0.8",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@10.8.2"
}
