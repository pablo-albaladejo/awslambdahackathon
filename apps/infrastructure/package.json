{
  "name": "@awslambdahackathon/infrastructure",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy",
    "deploy:dev": "npm run build && cdk deploy --context environment=dev",
    "deploy:prod": "npm run build && cdk deploy --context environment=prod",
    "deploy:ci": "npm run build && cdk deploy --require-approval never",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "bootstrap": "cdk bootstrap",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-nodejs": "^1.204.0",
    "aws-cdk-lib": "^2.202.0",
    "constructs": "^10.0.0",
    "esbuild": "^0.23.0",
    "fs-extra": "^11.2.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.2",
    "aws-cdk": "2.146.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  }
}
